[Journal]
Storage=<%= @storage %>
Compress=<%= @compress %>
Seal=<%= @seal %>
SplitMode=<%= @split_mode %>
SyncIntervalSec=<%= @sync_interval_sec %>
RateLimitInterval=<%= @rate_limit_interval %>
RateLimitBurst=<%= @rate_limit_burst %>
#SystemMaxUse=
#SystemKeepFree=
#SystemMaxFileSize=
#RuntimeMaxUse=
#RuntimeKeepFree=
#RuntimeMaxFileSize=
#MaxRetentionSec=
MaxFileSec=<%= @max_file_sec %>
ForwardToSyslog=<%= @forward_to_syslog %>
ForwardToKMsg=<%= @forward_to_kmsg %>
ForwardToConsole=<%= @forward_to_console %>
ForwardToWall=<%= @forward_to_wall %>
TTYPath=<%= @tty_path %>
#MaxLevelStore=debug
#MaxLevelSyslog=debug
#MaxLevelKMsg=notice
#MaxLevelConsole=info
#MaxLevelWall=emerg
